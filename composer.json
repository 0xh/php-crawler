        "crawler",
        "spider",
        "php crawler"
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "pimple/pimple": "^3.0",
        "phpunit/phpunit": "^6.3",
        "monolog/monolog": "^1.23",
        "querypath/querypath": "^3.0"
    },
    "autoload": {
